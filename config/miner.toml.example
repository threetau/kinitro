version = "0.1.0"
log_level = "INFO" # Must be one of TRACE, DEBUG, INFO, WARN, ERROR

wallet_path = "~/.bittensor/wallets"
wallet_name = "default"
hotkey_name = "default"

# Backend API used for requesting upload slots and reporting results
backend_url = "https://api.kinitro.ai"

# Submission configuration
submission_dir = "./evaluator/submissions/default_submission"
# Optional: uncomment to override auto-generated version or hold-out window
# submission_version = "v1"

# Chosen competition ID
competition_id = "your-chosen-competition-id"

[subtensor]
network = "finney"
address = "wss://entrypoint-finney.opentensor.ai:443"
netuid = 26
