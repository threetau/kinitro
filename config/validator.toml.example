version = "0.0.1"
log_level = "INFO" # Must be one of TRACE, DEBUG, INFO, WARN, ERROR

wallet_path = "~/.bittensor/wallets"
wallet_name = "default"
hotkey_name = "default"

# Database configuration
pg_database = "postgresql://postgres@localhost/postgres"
duckdb_path = "evaluation_data.duckdb"  # Path for DuckDB evaluation data storage

# Parent-Child Validator Configuration
# Set is_parent to true for parent validators that discover jobs from chain
# Set is_parent to false for child validators that receive jobs from parent
is_parent = false

# Parent validator WebSocket server settings (only used when is_parent = true)
broadcast_host = "0.0.0.0"  # Host to bind WebSocket server
broadcast_port = 8765       # Port for WebSocket server

# Child validator connection settings (only used when is_parent = false)
parent_host = "localhost"   # Parent validator hostname or IP
parent_port = 8765         # Parent validator WebSocket port

[subtensor]
network = "finney"
address = "wss://entrypoint-finney.opentensor.ai:443"
netuid = 26

[neuron]
sync_frequency = 300
load_old_nodes = true
min_stake_threshold = 1000
allowed_validators = []
max_commitment_lookback = 360 # Maximum number of recent blocks to scan per sync

[otel]
api_key = ""
endpoint = ""
service_name = ""
