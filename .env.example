# Kinitro Configuration
# Copy this file to .env and fill in your values

# NETWORK CONFIGURATION
# -----------------------------------------------------------------------------
NETWORK=finney              # finney (mainnet), test (testnet), or local
NETUID=                     # Your subnet's netuid (REQUIRED)

# WALLET CONFIGURATION (for validators)
# -----------------------------------------------------------------------------
WALLET_NAME=default         # Name of your Bittensor wallet
HOTKEY_NAME=default         # Name of your hotkey

# DATABASE CONFIGURATION (for backend)
# -----------------------------------------------------------------------------
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=kinitro

# API KEYS
# -----------------------------------------------------------------------------
BASILICA_API_TOKEN=         # Token for Basilica container execution
CHUTES_API_KEY=             # API key for Chutes (SN64) inference

# EVALUATION SETTINGS
# -----------------------------------------------------------------------------
EPISODES_PER_ENV=50         # Episodes to run per environment per cycle
EVAL_INTERVAL=3600          # Seconds between evaluation cycles (1 hour)

# LOGGING
# -----------------------------------------------------------------------------
LOG_LEVEL=INFO              # DEBUG, INFO, WARNING, ERROR

# MONITORING (Optional)
# -----------------------------------------------------------------------------
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin
